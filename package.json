{
  "name": "@storybook/testing-angular",
  "version": "1.0.3",
  "description": "Testing utilities that allow you to reuse your stories in your unit tests",
  "keywords": [
    "storybook-addons",
    "style",
    "test"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/storybookjs/testing-angular"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "engines": {
    "node": ">=18.19.1"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "release": "yarn build && auto shipit"
  },
  "peerDependencies": {
    "@angular/common": ">=16.0.0",
    "@angular/core": ">=16.0.0",
    "@angular/platform-browser": ">=16.0.0",
    "@storybook/angular": ">=8.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "markberry867@gmail.com",
  "module": "dist/testing-angular.esm.js",
  "size-limit": [
    {
      "path": "dist/testing-angular.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/testing-angular.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@angular/common": "~18.0.0",
    "@angular/core": "~18.0.0",
    "@angular/platform-browser": "~18.0.0",
    "@size-limit/preset-small-lib": "^8.2.4",
    "auto": "^11.1.6",
    "concurrently": "^8.1.0",
    "husky": "^9.0.11",
    "size-limit": "^11.1.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@storybook/angular": "^8.1.5",
    "@storybook/preview-api": "^8.1.5",
    "@storybook/types": "^8.1.5"
  },
  "resolutions": {
    "**/typescript": "^5.4.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
